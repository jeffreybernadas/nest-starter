{
  "name": "nest-starter",
  "version": "0.0.1",
  "description": "A personal, production-ready NestJS starter template with enterprise-grade features.",
  "author": {
    "name": "Jeffrey Bernadas",
    "email": "jeffrey.bernadas0@gmail.com",
    "url": "https://jeffreybernadas.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jeffreybernadas/nest-starter"
  },
  "license": "MIT",
  "scripts": {
    "postinstall": "npx prisma generate",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:worker": "nest start --watch --entryFile main.worker",
    "start:worker:dev": "nest start --watch --entryFile main.worker",
    "start:worker:debug": "nest start --debug --watch --entryFile main.worker",
    "start:worker:prod": "node dist/main.worker",
    "email:dev": "email dev --dir ./src/shared/mail/templates --port 3002",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "docker:start": "docker-compose up -d",
    "db:seed": "npx prisma db seed",
    "db:studio": "npx prisma studio",
    "db:migrate:dev": "npx prisma migrate dev",
    "db:migrate:prod": "npx prisma migrate deploy",
    "prepare": "husky"
  },
  "prisma": {
    "seed": "tsx src/database/seed.ts",
    "schema": "src/database/schema.prisma"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^9.1.1",
