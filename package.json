    "husky": "^9.1.7",
    "jest": "^30.0.0",
    "lint-staged": "^16.2.0",
    "prettier": "^3.4.2",
    "prisma": "^6.16.2",
    "react-email": "4.2.11",
    "socket.io-client": "^4.8.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
