{
  "name": "nest-starter",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "postinstall": "npx prisma generate",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "email:dev": "email dev --dir ./src/shared/mail/templates --port 3002",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "docker:start": "docker-compose up -d",
    "db:seed": "npx prisma db seed",
    "db:studio": "npx prisma studio",
    "db:migrate:dev": "npx prisma migrate dev",
    "db:migrate:prod": "npx prisma migrate deploy",
    "prepare": "husky"
  },
  "prisma": {
    "seed": "tsx src/database/seed.ts",
    "schema": "src/database/schema.prisma"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^9.1.1",
    "@golevelup/ts-jest": "^0.7.0",
    "@nest-lab/throttler-storage-redis": "^1.1.0",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/throttler": "^6.4.0",
    "@prisma/client": "^6.16.2",
    "@prisma/extension-accelerate": "^2.0.2",
    "@react-email/components": "0.5.5",
    "axios": "^1.12.2",
    "cache-manager": "^7.2.2",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dotenv": "^17.2.2",
    "elastic-apm-node": "^4.13.0",
    "helmet": "^8.1.0",
    "http-terminator": "^3.2.0",
    "ioredis": "^5.8.0",
    "lodash": "^4.17.21",
    "nestjs-graceful-shutdown": "^1.0.3",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "reflect-metadata": "^0.2.2",
    "resend": "^6.1.0",
    "rxjs": "^7.8.1",
    "tslib": "^2.8.1",
    "winston": "^3.17.0",
    "winston-elasticsearch": "^0.19.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@react-email/preview-server": "4.2.11",
    "@types/cache-manager-redis-store": "^2.0.4",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@types/node": "^22.10.7",
    "@types/react": "^19.1.13",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jest": "^30.0.0",
    "lint-staged": "^16.2.0",
    "prettier": "^3.4.2",
    "prisma": "^6.16.2",
    "react-email": "4.2.11",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.5",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "!**/*.spec.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/database/(factories|migrations|seeds)/",
      "<rootDir>/main.ts",
      "(.+).module.ts"
    ],
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/common(.*)$": "<rootDir>/common$1",
      "^@/config(.*)$": "<rootDir>/config$1",
      "^@/constants(.*)$": "<rootDir>/constants$1",
      "^@/database(.*)$": "<rootDir>/database$1",
      "^@/decorators(.*)$": "<rootDir>/decorators$1",
      "^@/exceptions(.*)$": "<rootDir>/exceptions$1",
      "^@/filters(.*)$": "<rootDir>/filters$1",
      "^@/generated(.*)$": "<rootDir>/generated$1",
      "^@/guards(.*)$": "<rootDir>/guards$1",
      "^@/interceptors(.*)$": "<rootDir>/interceptors$1",
      "^@/libs(.*)$": "<rootDir>/libs$1",
      "^@/shared(.*)$": "<rootDir>/shared$1",
      "^@/utils(.*)$": "<rootDir>/utils$1",
      "^@/tools(.*)$": "<rootDir>/tools$1"
    },
    "setupFiles": [
      "../setup-jest.mjs"
    ],
    "cacheDirectory": "../.jest-cache",
    "testTimeout": 120000
  }
}
