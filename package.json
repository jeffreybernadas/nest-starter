    "@sentry/nestjs": "^10.17.0",
    "@sentry/profiling-node": "^10.17.0",
    "@socket.io/redis-adapter": "^8.3.0",
    "axios": "^1.12.2",
    "cache-manager": "^7.2.2",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.5.1",
