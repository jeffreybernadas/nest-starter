    "dotenv": "^17.2.2",
    "elastic-apm-node": "^4.13.0",
    "helmet": "^8.1.0",
    "http-terminator": "^3.2.0",
    "ioredis": "^5.8.0",
    "lodash": "^4.17.21",
    "minio": "^8.0.6",
    "nest-keycloak-connect": "^1.10.1",
    "nestjs-graceful-shutdown": "^1.0.3",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "redis": "^5.8.3",
